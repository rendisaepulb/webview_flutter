<manifest package="io.flutter.plugins.webviewfluttercustom"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <application>
        <activity
            android:name="io.flutter.plugins.webviewfluttercustom.RequestCameraPermissionActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" />
        <activity
            android:name="io.flutter.plugins.webviewfluttercustom.FileChooserActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" />

        <provider
            android:name="io.flutter.plugins.webviewfluttercustom.GenericFileProvider"
            android:authorities="${applicationId}.generic.provider"
            android:exported="false"
            android:grantUriPermissions="true"
            >
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths"
                />
        </provider>
    </application>
</manifest>